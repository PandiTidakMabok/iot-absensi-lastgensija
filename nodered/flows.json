        "id": "371b48321a6ce361",
        "type": "function",
        "z": "32c26991b3ccfb96",
        "name": "tambah/perbarui data siswa",
        "func": "const state = flow.get('state')\nconst data = msg.payload\nif (state) {\n    msg.topic = `UPDATE siswa SET student_name='${data.student_name}', class=${data.class}, major='${data.major}' WHERE uid='${data.uid}'` \n} else {\n    msg.topic = `INSERT INTO siswa (uid, student_name, class, major) VALUE ('${data.uid}','${data.student_name}','${data.class}','${data.major}')`\n}\n\nreturn msg;",
