    {
        "id": "03b46d94dfb8ac3b",
        "type": "function",
        "z": "47af2fcbca79a95b",
        "name": "siswa ditemukan",
        "func": "const now = new Date()\nconst nama = flow.get('student_name')\nconst kelas = flow.get('class')\nlet status\n\nif (now.getDay() > 5) { \n    status = 'prei bolo'\n} else { \n    if (now.getHours() > 5 && now.getHours() <= 7) { \n        status = 'hadir'\n    } else if (now.getHours() > 7 && now.getHours() <= 15) { \n        status = 'terlambat'\n    } else if (now.getHours() > 15 && now.getHours() <= 20) { \n        status = 'pulang'\n    } else { \n        status = 'lu ngapain njir, gabut amat'\n    } \n} \n\nmsg.payload = { \n    status: \"berhasil\",\n    nama,\n    kelas,\n    status_absen: status\n}\n\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 1610,
        "y": 280,
        "wires": [
